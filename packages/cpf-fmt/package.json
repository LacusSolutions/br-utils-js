{
  "name": "@lacussoft/cpf-fmt",
  "version": "1.0.10",
  "description": "Basic function to format CPF (Brazilian ID document).",
  "author": "Julio L. Muller",
  "license": "MIT",
  "keywords": [
    "lacussoft",
    "cpf",
    "format",
    "formatting",
    "formatar",
    "mascara",
    "br"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliolmuller/cpf-utils-js.git"
  },
  "bugs": {
    "url": "https://github.com/juliolmuller/cpf-utils-js/issues"
  },
  "homepage": "https://github.com/juliolmuller/cpf-utils-js/main/packages/cpf-fmt#readme",
  "typings": "build/types.d.ts",
  "main": "build/index.js",
  "scripts": {
    "build:browser": "rm -rf dist/ && webpack --mode=production",
    "build:node": "rm -rf build/ && babel src/ --out-dir build/ && cp src/*.d.ts build/",
    "build": "npm run build:browser && npm run build:node",
    "lint": "eslint --fix src/**/*.js",
    "test": "jest",
    "prepublish": "npm run build && npm test"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "html-escaper": "^2.0.2",
    "num-only": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "babel-loader": "^8.2.2",
    "eslint": "^7.27.0",
    "eslint-config-lacussoft": "^2.0.1",
    "jest": "^27.0.3",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  }
}
